<% @events.each do |ue| -%>
  <% next if ue.event.blank? %>
  <%= "<div 'main_event_container_div' id='main_event_container_#{@events.index(ue)}' style='display:#{@events.index(ue) == 0 ? 'block' : 'none'}'>" %> 
  <%= "<div class='flyer_div_2' id='flyer_#{@events.index(ue)}'>" %>
    <%= "<img src='#{ue.event.flyer.file_path}#{ue.event.flyer.filename}' />" %> 
  <%= "</div>" %>    
  <%= "<div class='main_event_info_2' id='event_info_#{@events.index(ue)}' >" %>
  <div class='info_container'>
  </div>
  <%= "</div>" %>
  <div class='clear'></div>
  <%= "</div>" %>
<% end -%>
<div id='bottom_nav'>
  <ul id='bottom_nav_button_list'>
    <li id='guest_list' ><a href='#'></a></li>
    <li id='bot_nav_divider_guest_list' >&nbsp;</li>
    <li id='special_packages' ><a href='#'></a></li>
    <li id='bot_nav_divider_special_packages' >&nbsp;</li>
    <li id='venue_details' ><a href='#'></a></li>
  </ul>
</div>
