    <div id='overlay' class='overlay'></div>
    <div id='banner' class='banner_class'></div>
    <div id='content' class='main_content'>
      <div id='top_nav' class ='top_nav'>
        <div id='top_nav_left' class='top_nav_left'></div>
        <div id='top_nav_mid' class='top_nav_mid'>
          <ul>
            <li id='home_link'><a href='/'></a></li>
            <li id='top_nav_divider_home'>&nbsp;</li>
            <li id='media_link'><a href='/media'></a></li>
            <li id='top_nav_divider_media'>&nbsp;</li>
            <li id='event_link'><a href='/plan_event'></a></li>
            <li id='top_nav_divider_event'>&nbsp;</li>
