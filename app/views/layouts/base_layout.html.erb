      <div id='footer_left_div'>PRIVACY POLICY</div>
      <div id='footer_right_div'>COPYRIGHT <%= Time.now.strftime('%Y') %> the Blaq List LLC All Rights Reserved</div>
    </div>
    <div class='clear' ></div>
  </body>
</html>
