            <li id='top_nav_divider_about'>&nbsp;</li>
            <li id='contact_link'><a href='/contact'></a></li>
            <li id='top_nav_divider_contact'>&nbsp;</li>
            <li id='follow_us'>&nbsp;</li>
            <li id='twitter_link'><a href='https://www.twitter.com/accessnightlife' title='Twitter' ></a></li>
            <li id='facebook_link'><a href='https://www.facebook.com/accessnightlife' title='Facebook' ></a></li>
            <li id='youtube_link'><a href='http://www.youtube.com/watch?v=NY4miE4mx2U' title='YouTube' ></a></li>
          </ul>
        </div>
        <div id='top_nav_right' class='top_nav_right'></div>
      </div>
      <div class='clear' ></div>
	   <%= yield %>
    </div>
